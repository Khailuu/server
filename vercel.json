{
  "version": 2,
  "builds": [
    {
      "src": "zalo/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "momo/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/zalo/(.*)",
      "dest": "/zalo/server.js"
    },
    {
      "src": "/momo/(.*)",
      "dest": "/momo/server.js"
    }
  ]
}
